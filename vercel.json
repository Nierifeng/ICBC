      {
        "version": 2,
        "builds": [
          {
            "src": "*.html",
            "use": "@vercel/static"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "/$1"
          },
          {
            "handle": "filesystem"
          },
          {
            "src": "/(.*)\\.(js|css|jpg|jpeg|png|gif|ico|json|svg|woff|woff2|ttf|eot|webp)",
            "headers": { "cache-control": "public,max-age=31536000,immutable" },
            "continue": true
          }
        ]
      }